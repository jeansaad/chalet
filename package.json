{
  "name": "chalet",
  "version": "0.9.0-1",
  "description": "Local domains for everyone and more! ",
  "main": "lib",
  "bin": "lib/cli/bin.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "ava && npm run lint",
    "lint": "eslint . --ignore-path .gitignore && stylelint './src/app/**/*.css'",
    "lint:fix": "eslint . --ignore-path .gitignore --fix && stylelint './src/app/**/*.css' --fix",
    "start": "run-p start:*",
    "start:webpack": "webpack-dev-server --config webpack.dev.js --open",
    "start:nodemon": "nodemon -- src/daemon",
    "prepublishOnly": "npm run build && pkg-ok",
    "uninstall": "node bin/uninstall.js",
    "build": "run-s build:*",
    "build:webpack": "rimraf dist && webpack --config webpack.prod.js",
    "build:babel": "rimraf lib && babel src -d lib --copy-files --ignore src/app",
    "addFixtures": "node src/cli/bin add \"node index\" --dir ./test/fixtures/app && node src/cli/bin add \"node index\" --dir ./test/fixtures/verbose && node src/cli/bin add http://some-domain.com --name local-domain "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeansaad/chalet.git"
  },
  "keywords": [
    "dev",
    "devtool",
    "domain",
    "host",
    "https",
    "local",
    "localhost",
    "manager",
    "process",
    "proxy",
    "server"
  ],
  "author": "Typicode <typicode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeansaad/chalet/issues"
  },
  "homepage": "https://github.com/jeansaad/chalet",
  "dependencies": {
    "@babel/register": "^7.8.6",
    "after-all": "^2.0.2",
    "ansi-to-html": "^0.6.14",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "connect-sse": "^1.2.0",
    "exit-hook": "^2.2.0",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "http-proxy": "^1.18.0",
    "matcher": "^2.1.0",
    "mkdirp": "^1.0.3",
    "once": "^1.3.2",
    "please-upgrade-node": "^3.2.0",
    "pug": "^2.0.4",
    "respawn": "^2.6.0",
    "selfsigned": "^1.10.7",
    "server-ready": "^0.3.1",
    "sudo-block": "^3.0.0",
    "tinydate": "^1.2.0",
    "unquote": "^1.1.1",
    "user-startup": "^0.2.1",
    "vhost": "^3.0.2",
    "yargs": "^15.3.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@types/classnames": "^2.2.10",
    "@types/escape-html": "0.0.20",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "ansi-html": "0.0.7",
    "ava": "^3.5.0",
    "classnames": "^2.2.6",
    "css-loader": "^3.4.2",
    "escape-html": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "lodash.uniqueid": "^4.0.1",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pkg-ok": "^2.3.1",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-standard": "^20.0.0",
    "supertest": "^4.0.2",
    "tempy": "^0.5.0",
    "terser-webpack-plugin": "^2.3.5",
    "tildify": "^2.0.0",
    "ts-loader": "^6.2.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.2.0",
    "typescript": "^3.8.3",
    "untildify": "^4.0.0",
    "update-notifier": "^4.1.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
